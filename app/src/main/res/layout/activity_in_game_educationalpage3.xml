<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".in_game_educationalpage3">

    <TextView
        android:layout_width="350dp"
        android:layout_height="500dp"
        android:background="@drawable/bt_box"
        android:fontFamily="sans-serif-condensed"
        android:text="Educational Information"
        android:textAlignment="center"
        android:textSize="45sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.58" />

    <ImageView
        android:id="@+id/edu_back"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginTop="548dp"
        android:layout_marginEnd="308dp"
        android:clickable="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/back_icon" />

    <ImageView
        android:id="@+id/edu_next"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginTop="548dp"
        android:layout_marginEnd="56dp"
        android:clickable="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/next_icon" />

    <ImageView
        android:id="@+id/edu_page3A"
        android:layout_width="0dp"
        android:layout_height="49dp"
        android:layout_marginTop="560dp"
        android:layout_marginEnd="228dp"
        android:clickable="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bt_circle_white_fill" />

    <ImageView
        android:id="@+id/edu_page3B"
        android:layout_width="0dp"
        android:layout_height="49dp"
        android:layout_marginTop="560dp"
        android:layout_marginEnd="200dp"
        android:clickable="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bt_circle_white_fill" />

    <ImageView
        android:id="@+id/edu_page3C"
        android:layout_width="0dp"
        android:layout_height="49dp"
        android:layout_marginTop="560dp"
        android:layout_marginEnd="172dp"
        android:clickable="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bt_circle_black_fill" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/hbg_menu_layout"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/hbg_menu1"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp"
            android:clickable="true"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/hbg_menu_icon" />

        <ImageView
            android:id="@+id/home_icon"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="344dp"
            android:clickable="true"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/home_icon" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>